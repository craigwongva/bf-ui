{
  "plugins": ["react"],
  "ecmaVersion": 7,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": ["plugin:react/recommended"],
  "rules": {
    "comma-spacing": 1,
    "constructor-super": 1,
    "complexity": 0,
    "curly": 1,
    "eqeqeq": 1,
    "indent": [1, 2, {"VariableDeclarator": 2, "SwitchCase": 1}],
    "new-cap": 1,
    "no-alert": 1,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-cond-assign": [1, "except-parens"],
    "no-const-assign": 2,
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 1,
    "no-extend-native": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-irregular-whitespace": 2,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-spaced-func": 1,
    "no-sparse-arrays": 1,
    "no-this-before-super": 2,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "no-use-before-define": [2, {"functions": false, "classes": false}],
    "no-useless-concat": 1,
    "prefer-const": 1,
    "quotes": [1, "single", "avoid-escape"],
    "semi": [1, "never"],
    "semi-spacing": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": [1, {"anonymous": "always", "named": "never"}],
    "space-in-parens": [1, "never"],
    "valid-typeof": 1
  }
}
